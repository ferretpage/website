<header class="py-1 mb-2 border-bottom">
      <div id="errorS" class="" role="alert" style="max-width: max-content; margin: auto;"></div>
      <div class="container-fluid d-grid gap-3 align-items-center" style="grid-template-columns: 85fr 2fr;">
        <div class="dropdown">
          <a href="/" class="d-flex align-items-center col-lg-4 mb-2 mb-lg-0 link-dark text-decoration-none dropdown-toggle font-bold dark:text-white" id="dropdownNavLink" data-bs-toggle="dropdown" aria-expanded="false">Ferret</a>
          <ul class="dropdown-menu text-small shadow" aria-labelledby="dropdownNavLink">
            <li><a class="dropdown-item" href="/">Home</a></li><% if (acc) {%>
            <li><a class="dropdown-item" href="/analytics">Analytics</a></li><%}%>
            <li><hr class="dropdown-divider"></li>
            <li><a class="dropdown-item" href="/help">Help</a></li>
            <li><a class="dropdown-item disabled" href="#">Display</a></li>
          </ul>
        </div>
      
        <div class="d-flex align-items-center">
          <div class="flex-shrink-0 dropdown">
            <a class="d-block link-dark text-decoration-none" id="dropdownUser2" data-bs-toggle="dropdown" aria-expanded="false"><% if (!acc) {%>
              <svg xmlns="http://www.w3.org/2000/svg" width="32" height="32" fill="currentColor" class="bi bi-person-fill" viewBox="2 0 14 14">
                <path d="M3 14s-1 0-1-1 1-4 6-4 6 3 6 4-1 1-1 1H3zm5-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6z"/>
              </svg><%} else {%>
              <img src="/avatar/<%= acc.uuid %>" class="rounded-full w-[32px] h-[32px] object-cover" draggable="false"><%}%>
            </a>
            <ul class="dropdown-menu text-small shadow" aria-labelledby="dropdownUser2"><% if (!acc) {%>
              <li><a class="dropdown-item" href="/register">Register</a></li>
              <li><a class="dropdown-item" href="/signin">Sign In</a></li><%} else {%>
              <li><a class="dropdown-item" href="/<%=acc.name%>">My Profile <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" fill="currentColor" class="bi bi-person-fill inline-flex m-auto align-middle" viewBox="0 0 16 16"><path d="M3 14s-1 0-1-1 1-4 6-4 6 3 6 4-1 1-1 1H3Zm5-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6Z"/></svg></a></li>
              <li><a class="dropdown-item" href="/dashboard">Dashboard <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" fill="currentColor" class="bi bi-sticky-fill inline-flex m-auto align-middle" viewBox="0 0 16 16"><path d="M2.5 1A1.5 1.5 0 0 0 1 2.5v11A1.5 1.5 0 0 0 2.5 15h6.086a1.5 1.5 0 0 0 1.06-.44l4.915-4.914A1.5 1.5 0 0 0 15 8.586V2.5A1.5 1.5 0 0 0 13.5 1h-11zm6 8.5a1 1 0 0 1 1-1h4.396a.25.25 0 0 1 .177.427l-5.146 5.146a.25.25 0 0 1-.427-.177V9.5z"/></svg></a></li>
              <li><a class="dropdown-item" href="/settings">Settings <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" fill="currentColor" class="bi bi-gear-fill inline-flex m-auto align-middle" viewBox="0 0 16 16"><path d="M9.405 1.05c-.413-1.4-2.397-1.4-2.81 0l-.1.34a1.464 1.464 0 0 1-2.105.872l-.31-.17c-1.283-.698-2.686.705-1.987 1.987l.169.311c.446.82.023 1.841-.872 2.105l-.34.1c-1.4.413-1.4 2.397 0 2.81l.34.1a1.464 1.464 0 0 1 .872 2.105l-.17.31c-.698 1.283.705 2.686 1.987 1.987l.311-.169a1.464 1.464 0 0 1 2.105.872l.1.34c.413 1.4 2.397 1.4 2.81 0l.1-.34a1.464 1.464 0 0 1 2.105-.872l.31.17c1.283.698 2.686-.705 1.987-1.987l-.169-.311a1.464 1.464 0 0 1 .872-2.105l.34-.1c1.4-.413 1.4-2.397 0-2.81l-.34-.1a1.464 1.464 0 0 1-.872-2.105l.17-.31c.698-1.283-.705-2.686-1.987-1.987l-.311.169a1.464 1.464 0 0 1-2.105-.872l-.1-.34zM8 10.93a2.929 2.929 0 1 1 0-5.86 2.929 2.929 0 0 1 0 5.858z"/></svg></a></li><% if (acc && acc.staff) {%>
              <li><hr class="dropdown-divider"></li>
              <li><a class="dropdown-item" href="/admin/panel">Admin Panel</a></li><%}%>
              <li><hr class="dropdown-divider"></li><% if (acc && acc.pro) {%>
              <li><a class="dropdown-item" href="/settings/switch_account">Switch User</a></li>
              <li><hr class="dropdown-divider"></li><%}%>
              <li><a class="dropdown-item" href="/signout">Sign out</a></li><%}%>
            </ul>
          </div>
        </div>
      </div>
    </header>